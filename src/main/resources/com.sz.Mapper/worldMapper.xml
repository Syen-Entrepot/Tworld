<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sz.Mapper.worldMapper">
<insert id="insert">
    insert into world (country,city) values(#{country},#{city})
</insert>
    <select id="queryById" resultType="com.sz.pojo.world">
       select * from world where id=#{id}
    </select>

</mapper>